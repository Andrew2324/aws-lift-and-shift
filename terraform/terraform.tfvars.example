# Copy this file to terraform.tfvars and fill in real values.
# DO NOT commit terraform.tfvars (contains sensitive info like your IP).

aws_region    = "us-east-1"
instance_type = "t3.micro"

# Must exist already in EC2 > Key Pairs
key_name   = "liftshift-key"

# Your public IP in /32 format (example)
admin_cidr = "203.0.113.10/32"
